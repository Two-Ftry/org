{
  "appenders": [
    {
      "category": "console",
      "type": "dateFile",
      "filename": "./log/logger-console",
      "maxLogSize": 104800,
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncluePattern": true
    },
    {
      "category": "logInfo",
      "type": "dateFile",
      "filename": "./log/logger-info",
      "maxLogSize": 104800,
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncluePattern": true
    },
    {
      "category": "logDate",
      "type": "dateFile",
      "filename": "./log/logger",
      "maxLogSize": 104800,
      "pattern": "-yyyy-MM-dd.log",
      "alwaysIncluePattern": true
    }
  ],
  "replaceConsole": true,
  "levels":{
    "console": "ALL",
    "logInfo": "ALL",
    "logDate": "WARN"
  }
}
